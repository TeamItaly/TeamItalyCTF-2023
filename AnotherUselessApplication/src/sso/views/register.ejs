<%- include('header') %>

<form class="form-group" method="POST">
    <input id="username" class="form-control my-2" name="username" placeholder="Username" minlength="4"/>
    <input id="password" class="form-control my-2" type="password" name="password" placeholder="Password" minlength="5"/>
    <button id="submit" class="btn btn-dark w-100 my-2" type="submit">Register</button>
</form>